<html>
<head>
	<title>This is js demo</title>
	<style>
		<!-- This is for css -->
	</style>
	<script>
	<!-- This is for java script function -->
		
		var a = 10;
		var b = 10;
		var result = a + b;
		alert("Result of + :" + result);
		
		var a = 20;
		var b = 10;
		var result = a - b;
		alert("Result of - :" + result);
		
		var a = 20;
		var b = 10;
		var result = a * b;
		alert("Result of * :" + result);
	
		// modules in javascript where we can wrap our repeatitive code
		// function is nothing but grouping of lines into a single modules
		// using this you can remove duplicate code and call as many times you want.
		
		// method (class oops) and function 
		
		// function structure
		function sum(){
			var a = 10;
			var b = 10;
			var result = a + b;
			alert("Result of + :" + result);
		}
		sum();

		function sum1(x, y){
			var a = x;
			var b = y;
			var result = a + b;
			alert("Result of + :" + result);
		}
		sum1(10, 20);
		sum1(100, 200);
	
	
	
	
	</script>
</head>

<body>

</body>
</html>